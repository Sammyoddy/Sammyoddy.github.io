row<-str_remove(row,"lbs")
row<- round(as.numeric(row)/2.20462,digits=0)
Weight_kg[counter]<-row
}
else{
row<-str_remove(row,"kg")
row<- as.numeric(row)
Weight_kg[counter]<-row
}
counter<-counter+1
}
return(Weight_kg)
}
